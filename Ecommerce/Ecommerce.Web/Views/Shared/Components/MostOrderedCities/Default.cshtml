@model List<CityOrderCountDto>
<div class="wg-box">
    <div class="flex items-center justify-between">
        <h5>En Çok Sipariş Veren Şehirler</h5>
    </div>

    <ul class="flex flex-column justify-between gap10 h-full">
        @foreach (var item in Model)
        {
            <li class="country-item">
                <div class="image">
                    <img src="~/admin_assets/images/country/6.png" alt="">
                </div>
                <div class="flex-grow flex items-center justify-between">
                    <a href="countries.html" class="body-text name">@item.City</a>
                    <div class="box-icon-trending up">
                        <i class="icon-trending-up"></i>
                    </div>
                    <div class="body-text number">@item.OrderCount</div>
                </div>
            </li>
        }
    </ul>
</div>